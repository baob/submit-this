{
    "env": {
        "node": true,
        "commonjs": true,
        "es6": false
      },
    "extends": [
      "eslint:recommended"
    ],
    "globals": {},
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "settings": {
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-extra-semi": 0
    }
}
